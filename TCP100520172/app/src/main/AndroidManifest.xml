<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jason.tcpdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ico"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ico"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".funcs.FuncTcpClient" />
        <activity android:name=".funcs.FuncTcpServer" />
        <activity
            android:name=".funcs.StopActivity"
            android:label="@string/title_activity_stop" />
        <activity
            android:name=".SetActivity"
            android:label="@string/title_activity_set" />
        <activity
            android:name=".funcs.AdminiActivity"
            android:label="@string/title_activity_admini" />
        <activity
            android:name=".funcs.SituaActivity"
            android:label="@string/title_activity_situa"></activity>
    </application>

</manifest>